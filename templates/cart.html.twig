{#
/**
 * Cart template
 */
#}
<div class="cart block container-fluid">
   <div class="cart-header row"> {{ title }}</div>
<div class="content clearfix">
    <div class="image-side col-sm-5 {{ color }}" >
        <i class="fa {{fa }} fa-lg"></i>
    </div>


    <div class="result-side col-md-7">
        {% for item in items %}
            <div class="item">-&gt; {{ item }}</div>
        {% endfor %}
    </div>

</div>

</div>